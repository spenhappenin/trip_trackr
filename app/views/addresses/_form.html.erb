<h1> Create New Address </h1>
<%= form_for @address do |f| %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br />

  <%= f.label :state %>
  <%= f.text_field :state %>
  <br />

  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <br />

  <%= f.label :country %>
  <%= f.text_field :country %>
  <br />
   

  <%= f.submit %>
<% end %>

